        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	if(current.status == "Draft"){
		current.state = '1';
		current.active = 'true';
	}
	else if(current.status == "Remediation In-progress"){
		current.state = '2';
		current.active = 'true';
	}
	else if(current.status == "Remediation Complete"){
		current.state = '3';
		current.active = 'false';
	}
	else if(current.status == "Cancelled"){
		current.state = '7';
		current.active = 'false';
	}
	current.update();
})(current, previous);]]></script>
